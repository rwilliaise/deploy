{
  "multipart": [
    {   "apply": { "model": "deploy:block/cable" }},
    {
      "when": { "north": "true" },
      "apply": { "model": "deploy:block/cable_side", "uvlock": true }
    },
    {
      "when": { "east": "true" },
      "apply": { "model": "deploy:block/cable_side", "y": 90, "uvlock": true }
    },
    {
      "when": { "south": "true" },
      "apply": { "model": "deploy:block/cable_side", "y": 180, "uvlock": true }
    },
    {
      "when": { "west": "true" },
      "apply": { "model": "deploy:block/cable_side", "y": 270, "uvlock": true }
    },
    {
      "when": { "up": "true" },
      "apply": { "model": "deploy:block/cable_side", "x": -90, "uvlock": true }
    },
    {
      "when": { "down": "true" },
      "apply": { "model": "deploy:block/cable_side", "x": 90, "uvlock": true }
    }
  ]
}